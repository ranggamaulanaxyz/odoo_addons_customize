<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_product_digital_token_form" model="ir.ui.view">
        <field name="name">view.product.digital.token.form</field>
        <field name="model">product.digital.token</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="partner_id" />
                            <field name="identifier" />
                        </group>
                        <group>
                            <field name="provider_id"/>
                            <field name="token" />
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_product_digital_token_list" model="ir.ui.view">
        <field name="name">view.product.digital.token.list</field>
        <field name="model">product.digital.token</field>
        <field name="arch" type="xml">
            <list>
                <field name="identifier" />
                <field name="partner_id" />
                <field name="provider_id" />
                <field name="token" />
            </list>
        </field>
    </record>

    <record id="action_product_digital_token" model="ir.actions.act_window">
        <field name="name">Product Digital Token</field>
        <field name="res_model">product.digital.token</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>